{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/.*", "dest": "https://tu-backend-en-docker.com" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }